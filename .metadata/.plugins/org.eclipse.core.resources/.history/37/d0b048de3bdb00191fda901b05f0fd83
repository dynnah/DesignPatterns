package Model;

public class PontuacaoController {
	private Pontuacao ponto;
	private Inimigo soldado;
	private Inimigo espiao;
	private Inimigo exercito;
	
	public PontuacaoController() {
		ponto = new Pontuacao();
		soldado = new Soldado();
		espiao = new Espiao();
		exercito = new Exercito();
	}
}
