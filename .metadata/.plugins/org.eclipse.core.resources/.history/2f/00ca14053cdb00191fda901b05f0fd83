	private Inimigo espiao;
	private Inimigo exercito;

	public PontuacaoController() {
		ponto = new Pontuacao();
		soldado = new Soldado();
		espiao = new Espiao();
		exercito = new Exercito();
	}	
	public String Ataque(String inimigo) {
		String result = "Ataque " + inimigo;
	}
}
}
